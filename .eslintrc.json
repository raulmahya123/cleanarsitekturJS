{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "warn",
            {
              "bracketSpacing": true
            }
          ],
         "no-async-promise-executor": "off",
            "require-jsdoc": "off",
          "no-console": "error",
          "no-var": "error"
    }
}
