{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "warn",
            {
              "bracketSpacing": true
            }
          ],
         "no-async-promise-executor": "off",
            "require-jsdoc": "off",
          "no-console": "error",
          "no-var": "error",
        "eqeqeq": "warn"
    }
}
